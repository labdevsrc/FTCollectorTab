<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FTCollectorApp.View.AsBuiltDocMenu">
    <ContentPage.Content>
        <StackLayout Padding="20">
            <Label Text="As Built Documents" FontSize="Large" HorizontalOptions="Center" Padding="20"/>

            <Button x:Name="btnSite" Text="Site" Clicked="btnSite_Clicked"  HorizontalOptions="FillAndExpand" Padding="20"/>
            <Button x:Name="btnTraceMenu" Text="Trace Fiber" Clicked="OpenTraceMenu"  HorizontalOptions="FillAndExpand" Padding="20"/>
            <Button Text="Delete Chassis"  HorizontalOptions="FillAndExpand" Padding="20"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>